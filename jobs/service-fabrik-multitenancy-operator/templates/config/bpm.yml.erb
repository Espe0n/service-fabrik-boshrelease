---
<%
CONFIG_PATH = "/var/vcap/jobs/service-fabrik-multitenancy-operator/config"
%>
processes:
- name: service-fabrik-multitenancy-operator
  executable: /var/vcap/packages/node/bin/node
  args:
  - /var/vcap/packages/service-fabrik-broker/operators/StartMultitenancyOperators.js
  env:
    CONF_DIR: <%= CONFIG_PATH %>
    NODE_ENV: production
    SETTINGS_PATH: <%= CONFIG_PATH %>/settings.yml
  hooks:
    pre_start: /var/vcap/jobs/service-fabrik-multitenancy-operator/bin/pre_start
